{
    "name": "gitbook-plugin-password-pro",
    "description": "为 Gitbook 页面设置君子密码",
    "main": "index.js",
    "version": "0.0.1",
    "author": {
        "name": "suranyi",
        "email": "suranyi.sysu@gmail.com"
    },
    "keywords": [
        "gitbook",
        "gitbook-plugin",
        "password"
    ],
    "engines": {
        "gitbook": ">=2.0.0"
    },
    "homepage": "https://github.com/Zhangliubin/gitbook-plugin-password-pro",
    "repository": {
        "type": "git",
        "url": "https://github.com/Zhangliubin/gitbook-plugin-password-pro.git"
    },
    "gitbook": {
        "properties": {
            "tip": {
                "type": "string",
                "title": "[全局] 提示输入密码的文本信息",
                "default": "请输入该页面的访问密码:",
                "required": false
            },
            "password": {
                "type": "string",
                "title": "[全局] 保存密码信息",
                "required": false
            },
            "errorTip": {
                "type": "string",
                "title": "[全局] 密码输入错误时的提示信息",
                "default": "密码错误, 请重新输入:",
                "required": false
            },
            "reject": {
                "type": "string",
                "title": "[全局] 拒绝访问时的提示信息",
                "default": "当前页面拒绝访问: 身份验证失败.",
                "required": false
            }
        }
    },
    "dependencies": {
        "gitbook-plugin-password-pro": "^0.0.1"
    }
}
